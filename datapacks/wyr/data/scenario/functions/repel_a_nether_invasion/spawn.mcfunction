# joining team
team join rani_dead @e[tag=rani_enemy]

# adding spawn
scoreboard players add $spawned rani_timer 1
execute if score $spawned rani_timer matches 6.. if score $wave rani_timer matches ..9 run scoreboard players add $wave rani_timer 1
execute if score $spawned rani_timer matches 6.. run scoreboard players set $spawned rani_timer 0

# resetting timer
scoreboard players set $spawn_timer rani_timer 30


# sound and particle
playsound minecraft:block.netherite_block.break master @a ~ ~ ~ 1 0
playsound minecraft:entity.blaze.hurt master @a ~ ~ ~ 0.5 0

